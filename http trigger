module.exports = async function (context, req) {
    context.res = {
        // Status code
        status: 200,
        // Content type
        headers: {
            'Content-Type': 'application/json'
        },
        // Response body
        body: {
            message: 'This is sample data from the HTTP endpoint',
            data: [
                { id: 1, name: 'Sample Item 1', value: 100 },
                { id: 2, name: 'Sample Item 2', value: 200 }
            ]
        }
    };
};
